{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  // JWT Configurations
  "JwtOptions": {
    "Issuer": "https://localhost:7004",
    "Audience": "https://localhost:7004",

    // Used to encrypt and decrypt the jwt token
    "SigningKey": "some-signing-key-here+some-signing-key-here",

    // token expiration time
    "ExpirationSeconds": 3600
  }
}
